set(FORT_HERMES_CPP_VERSION @PROJECT_VERSION@)

find_package(Protobuf 3.0.0 REQUIRED)

@PACKAGE_INIT@

set_and_check(FORT_HERMES_CPP_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@" ${PROTOBUF_INCLUDE_DIRS})
set_and_check(FORT_HERMES_CPP_LIBRARIES "@PACKAGE_LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@fort-hermes-cpp@CMAKE_SHARED_LIBRARY_SUFFIX@" ${PROTOBUF_LIBRARIES} )

check_required_components(FortHermesCpp)
